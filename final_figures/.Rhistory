group_by(date, fire.year.bin) %>%
summarize(Tree_Cover.mean = mean(Tree_Cover), Tree_Cover.n = n()) %>%
filter(if_else(fire.year.bin == '1980-2010', Tree_Cover.n >= 3000, Tree_Cover.n >= 0)),
